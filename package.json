    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "turbo": "^1.13.3"
  },
  "dependencies": {
    "@radix-ui/react-switch": "^1.1.0"
  }
